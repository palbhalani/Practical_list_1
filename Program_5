// Write a C program to input elements in array and print all negative elements. 

#include<iostream>
using namespace std;

int main()
{
    int i,n,arr[100];

    cout<<"Enter the size of array:: ";
    cin>>n;
    cout<<"Enter array elements :: "<<endl;

    for (i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    
    cout<<endl;
    cout<<"Negative elements is :: "<<endl;
    
    for (i=0;i<n;i++)
    {
        if(arr[i]<0)
        {
            cout<<arr[i]<<endl;
        }
    }
}
