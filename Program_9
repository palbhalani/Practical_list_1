// Write C++ Program to Check Even or Odd Integers.

#include<iostream>
using namespace std;
main()
{
	int n;
	cout<<"My Name Is Bhalani Pal"<<endl;
    cout<<"En_no :: 220130318080 "<<endl;
	cout<<"Enter the number :: ";
	cin>>n;
	
	if(n%2==0)
	{
		cout<<"This is Even number ";
	}
	else
	{
		cout<<"This is odd number ";
	}
	return 0;
}
